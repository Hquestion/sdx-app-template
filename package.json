{
    "name": "skydiscovery",
    "version": "5.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "flow": "flow check",
        "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/assets/i18n/**/*.json'",
        "inspect:dev": "vue-cli-service inspect --mode development > build/dev_inspect_output.js",
        "inspect:prod": "vue-cli-service inspect --mode production > build/prod_inspect_output.js",
        "lint:nofix": "vue-cli-service lint --no-fix",
        "server:mock": "nodemon mock/bin/www",
        "test:unit": "vue-cli-service test:unit",
        "test:unit:coverage": "vue-cli-service test:unit --coverage",
        "link:sdx": "yarn link @sdx/ui @sdx/utils @sdx/widget @sdx/view"
    },
    "dependencies": {
        "@sdx/ui": "latest",
        "@sdx/utils": "latest",
        "@sdx/view": "latest",
        "@sdx/widget": "latest",
        "@skyide/services": "^1.1.4",
        "async-validator": "^1.11.2",
        "axios": "^0.18.0",
        "babel-polyfill": "^6.26.0",
        "clipboard": "^2.0.0",
        "co": "^4.6.0",
        "d3": "^5.5.0",
        "dayjs": "^1.8.14",
        "echarts": "4.2.1",
        "element-ui": "2.7.2",
        "feb-alive": "^1.0.7",
        "file-saver": "2.0.1",
        "lodash": "^4.17.4",
        "mockjs": "1.0.1-beta3",
        "moment": "^2.20.1",
        "normalize.css": "^8.0.1",
        "nprogress": "0.2.0",
        "portal-vue": "^2.1.6",
        "qs": "^6.7.0",
        "swiper": "^4.4.1",
        "vue": "^2.6.10",
        "vue-awesome-swiper": "^3.1.3",
        "vue-cookies": "^1.5.13",
        "vue-i18n": "^8.0.0",
        "vue-radial-progress": "^0.2.10",
        "vue-router": "^3.0.1",
        "vuescroll": "^4.12.2",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.4.3",
        "@babel/preset-flow": "^7.0.0",
        "@vue/cli-plugin-babel": "^3.1.1",
        "@vue/cli-plugin-eslint": "^3.1.1",
        "@vue/cli-plugin-unit-jest": "^3.3.0",
        "@vue/cli-service": "^3.1.1",
        "@vue/server-test-utils": "^1.0.0-beta.28",
        "@vue/test-utils": "^1.0.0-beta.20",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^24.7.1",
        "babel-plugin-component-customize": "^1.1.1",
        "babel-preset-flow": "^6.23.0",
        "eslint": "^5.8.0",
        "eslint-plugin-flowtype": "^3.2.1",
        "eslint-plugin-vue": "^5.0.0",
        "fast-sass-loader": "^1.5.0",
        "flow-bin": "^0.97.0",
        "flow-webpack-plugin": "^1.2.0",
        "node-sass": "^4.11.0",
        "nodemon": "^1.18.11",
        "sass-bem-constructor": "^1.1.0",
        "sass-loader": "^7.1.0",
        "sass-resources-loader": "^2.0.0",
        "vue-cli-plugin-i18n": "^0.6.0",
        "vue-server-renderer": "^2.6.10",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "componentDevEnv": true
}
